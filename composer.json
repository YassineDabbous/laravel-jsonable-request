{
    "version": "0.0.1",
    "name": "yassinedabbous/laravel-jsonable-request",
    "description": "Build http requests from Array/Json",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yassine Dabbous",
            "email": "yassine.dabbous@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": ["laravel", "http", "guzzle", "request", "json"],
    "require": {
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
      "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "YassineDabbous\\JsonableRequest\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "YassineDabbous\\JsonableRequest\\Tests\\": "tests/"
      }
    },
    "scripts": {
      "test": "vendor/bin/phpunit"
    }
}
